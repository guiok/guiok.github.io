<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.

broadcast chan mp;
hybrid clock lucro = 0.0;</declaration>
	<template>
		<name x="5" y="5">equip</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="0" y="0">
			<name x="-17" y="-42">FUNC</name>
			<label kind="invariant" x="-93" y="0">lucro'==1</label>
			<label kind="exponentialrate" x="-59" y="-25">1:100</label>
		</location>
		<location id="id1" x="136" y="0">
			<name x="127" y="-42">CORRETIVA</name>
			<label kind="invariant" x="126" y="17">lucro'==0.1</label>
			<label kind="exponentialrate" x="153" y="-17">1:10</label>
		</location>
		<location id="id2" x="25" y="153">
			<name x="34" y="110">PREVENTIVA</name>
			<label kind="invariant" x="15" y="170">lucro'==0.1</label>
			<label kind="exponentialrate" x="42" y="136">1:1</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id2"/>
			<target ref="id0"/>
			<nail x="34" y="76"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-51" y="51">mp?</label>
			<nail x="-25" y="85"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<nail x="68" y="17"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<nail x="68" y="-25"/>
		</transition>
	</template>
	<template>
		<name>manut</name>
		<declaration>clock t;</declaration>
		<location id="id3" x="0" y="0">
			<label kind="invariant" x="-10" y="17">t&lt;=1</label>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id3"/>
			<target ref="id3"/>
			<label kind="guard" x="-16" y="-85">t==1</label>
			<label kind="synchronisation" x="-16" y="-68">mp!</label>
			<label kind="assignment" x="-16" y="-51">t=0</label>
			<nail x="-34" y="-51"/>
			<nail x="59" y="-51"/>
		</transition>
	</template>
	<system>system equip, manut;</system>
	<queries>
		<query>
			<formula>E[&lt;=10; 100] (max: lucro)</formula>
			<comment></comment>
		</query>
		<query>
			<formula>simulate 10 [&lt;=10] {lucro}</formula>
			<comment></comment>
		</query>
		<query>
			<formula>Pr[&lt;=10] (&lt;&gt; equip.CORRETIVA)</formula>
			<comment></comment>
		</query>
		<query>
			<formula>A[] not deadlock</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
